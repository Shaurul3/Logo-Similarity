            var vueLoadedCallback = function () {
                if (loadSwiper) {
                    var swiperCSS = document.createElement('link');
                    swiperCSS.rel = "stylesheet";
                    swiperCSS.href = "https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css";
                    document.head.appendChild(swiperCSS);
                    // Load Swiper only after Vue is confirmed to be loaded
                    var swiperScript = document.createElement('script');
                    swiperScript.src = "https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js";
                    swiperScript.onload = callback; // Call the original callback after Swiper is loaded
                    document.head.appendChild(swiperScript);
                } else {
                    // If Swiper is not needed, call the callback immediately after Vue is loaded
                    callback();
                }
            };
    
            if (window.Vue) {
                // Vue is already loaded, proceed to check and load Swiper if needed
                vueLoadedCallback();
            } else {
                // Vue is not loaded, load it first
                var vueScript = document.createElement('script');
                vueScript.src = "https://unpkg.com/vue@3/dist/vue.global.prod.js";
                vueScript.onload = vueLoadedCallback; // After Vue is loaded, check and load Swiper if needed
                document.head.appendChild(vueScript);
            }
        };
    
        window.createVueApp = function ({ mountId, endpoint, appName, templateId, headerText,countryId=false,componentCampaignName="CC001"}) {
            const loadSwiperVue = templateId === "productSlider";
            function initializeApp() {
                const { createApp } = Vue;
    
    
                // Clone the template and insert it into the mounting div
                const template = document.getElementById(templateId).content.cloneNode(true);
                const mountElement = document.querySelector(mountId);
                mountElement.appendChild(template);
                
                
                const countryMapping = {
                    '3128': {
                        countryLocale: 'en_GB',
                        countryCurrency: 'ZAR'
                    },
                    '3177': {
                        countryLocale: 'en_GB',
                        countryCurrency: 'KES'
                    },
                    '3829': {
                        countryLocale: 'en_GB',
                        countryCurrency: 'NAD'
                    }
                    
                };
    
                const domainToCountryIdMapping = {
                    'co.za': 3128,
                    'com.na': 3829,
                    'co.ke':3177
                    
                };

                
                function getCountryIdFromDomain(domain) {
                    for (const key in domainToCountryIdMapping) {
                        if (domain.endsWith(key)) {
                            return domainToCountryIdMapping[key];
                        }
                    }
                    return null;
                }
                if(!countryId){
                    countryId = getCountryIdFromDomain(window.location.hostname)
                    if (countryId == null) {
                        throw new Error('Country ID could not be determined from the domain.');
                    }
                }   

                const app = createApp({
                    name: appName,
    
                    data() {
                        return {
    
                            products: [],
                            selectedProducts: [],
                            isLoading: true,
                            observedProducts: new Set(),
                            headerText: headerText,
                            fallbackImageUrl: "https://eshop.wurth.co.za/is-bin/intershop.static/WFS/3128-B1-Site/-/en_GB/webkit_bootstrap/dist/img/fallback-img/fallback-576px.jpg",
                            loggedIn: window.checkLoggedInState,
                            swiper: null,
                            mountId: mountId.replace('#', '')
    
    
                        };
                    },
    
                    methods: {
                        /* async fetchData() {
                            try {
                                const response = await fetch(endpoint);
                                const data = await response.json();
    
                                this.products = data.data.map((product) => ({
                                    TAG: product.Tag,
                                    Product_URL: product.Url_Product || '',
                                    Model_URL: product.Url_Model || '',
                                    Product_name: product.Product_name || '',
                                    Product_picture: product.Product_picture || '',
                                    Product_number: product.Product_number || '',
                                    Description: product.Description || '',
                                    Package_size: product.Package_size || '',
                                    Technical_description: product.Technical_description,
                                    Duplicate_Model: product.Duplicate_Model,
                                    WurthPrice: [],
                                    pricingLoaded: false,
                                    isVisible: false,
                                    beingObserverd: false,
                                }));
                                //Why did i need unique product numbers, and why am I not using it anywhere else
                                //const productNumbers = Array.from(new Set(this.products.map(product => product.Product_number)));
    
    
                                this.isLoading = false;
    
                            } catch (error) {
                                // console.error("Error fetching data:", error);
    
                            }
                        }, */
                        async fetchData() {
                            try {
                                const response = await fetch(endpoint);
                                const data = await response.json();
    
                                const productMap = new Map();
    
                                data.data.forEach((product) => {
                                    const modelURL = product.Url_Model || '';
    
                                    if (product.Display_Range === 'Yes' && productMap.has(modelURL)) {
                                        // If a duplicate is found for an existing model, ignore this product.
                                        return;
                                    }
    
                                    // Add or update the product in the map
                                    productMap.set(modelURL, {
                                        TAG: product.Tag,
                                        Product_URL: product.Url_Product || '',
                                        Model_URL: product.Url_Model,
                                        Product_name: product.Product_name || '',
                                        Product_picture: product.Product_picture || '',
                                        Product_number: product.Product_number || '',
                                        Description: product.Description || '',
                                        Package_size: product.Package_size || '',
                                        Technical_description: product.Technical_description,
                                        Duplicate_Model: product.Duplicate_Model,
                                        Display_Range: product.Display_Range,
                                        Display_Add_To_Cart: product.Display_Add_To_Cart,
                                        Product_Selected_Quantity: 1,
                                        WurthPrice: [],
                                        pricingLoaded: false,
                                        isVisible: false,
                                        beingObserverd: false,
                                    });
                                });
    
                                // Convert the map values back to an array
                                this.products = Array.from(productMap.values());
    
                                this.isLoading = false;
    
                            } catch (error) {
                                // console.error("Error fetching data:", error);
                            }
                        },
    
                        async fetchChunkedPricing(visibleProducts) {
                            const unloadedProducts = visibleProducts.filter(product => !product.pricingLoaded);
                            const productNumbers = unloadedProducts.map(product => product.Product_number.trim());
                            //console.log("Logged in state before pricing call",window.checkLoggedInState)
                            if (productNumbers.length === 0 || !window.checkLoggedInState) return;
    
                            try {
                                const response = await fetch(`/is-bin/INTERSHOP.enfinity/WFS/${countryId}-B1-Site/${countryMapping[countryId].countryLocale}/-/${countryMapping[countryId].countryCurrency}/ViewModelDetail-AjaxGetPrice?Products%5B%5D=${productNumbers.join('&Products%5B%5D=')}&SupplierID=WuerthGroup-Wuerth&ModelName=1`);
                                const priceData = await response.json();
    
                                // Create a map for price data based on product numbers
                                const priceDataMap = {};
                                priceData.forEach(item => {
                                    const productNumber = item.productNo.trim();
                                    priceDataMap[productNumber] = item.articlePriceVos;
                                });
    
                                // Map price data to products
                                unloadedProducts.forEach(product => {
                                    const productNumber = product.Product_number.trim();
                                    const matchingPriceData = priceDataMap[productNumber] || [];
                                    product.WurthPrice = matchingPriceData;
    
    
                                    // Set the smallest packing unit as the default, if applicable
                                    if (product.WurthPrice.length > 0) {
                                        let smallestPackingUnit = product.WurthPrice[0].packingUnit;
                                        product.WurthPrice.forEach(price => {
                                            if (price.packingUnit < smallestPackingUnit) {
                                                smallestPackingUnit = price.packingUnit;
                                            }
                                        });
                                        product.SelectedPackUnit = smallestPackingUnit;
                                    }
                                    product.pricingLoaded = true; // Mark as loaded
                                });
                            } catch (error) {
                                console.error("Error fetching prices:", error);
                                // Implement additional error handling or user feedback here
                            }
                        },
    
                        createIntersectionObserver() {
                            const observerOptions = {
                                root: null,
                                rootMargin: '700px 700px 700px 700px', // Adjust as needed
                                threshold: [0, 0.25, 0.5, 0.75, 1],
                            };
    
                            const observer = new IntersectionObserver((entries, observer) => {
                                const visibleProducts = [];
                                entries.forEach(entry => {
                                    if (entry.isIntersecting) {
                                        const product = this.products.find(p => `product-${p.Product_number}-${this.mountId}` === entry.target.id);
                                        //check if the product is found in the product array.
                                        if (product) {
                                            product.isVisible = true;
                                            if (!product.pricingLoaded) {
                                                visibleProducts.push(product);
                                            }
                                            if (product.pricingLoaded) {
                                                observer.unobserve(entry.target);
                                                product.beingObserverd = false;
                                            }
                                        }
                                    }
                                });
                                if (visibleProducts.length > 0) {
                                    this.fetchChunkedPricing(visibleProducts);
                                }
                            }, observerOptions);
    
                            this.products.forEach(product => {
                                const elementId = `product-${product.Product_number}-${this.mountId}`;
                                if (!this.observedProducts.has(elementId)) {
                                    const element = document.getElementById(elementId);
                                    if (element) {
                                        observer.observe(element);
                                        product.beingObserverd = true;
                                    }
                                }
                            });
                        },
                        debounce(func, wait) {
                            let timeout;
                            return function (...args) {
                                const context = this;
                                clearTimeout(timeout);
                                timeout = setTimeout(() => func.apply(context, args), wait);
                            };
                        },
                        checkInitialVisibility() {
                            this.products.forEach(product => {
                                const elementId = `product-${product.Product_number}-${mountId}`;
                                const element = document.getElementById(elementId);
                                if (element) {
                                    const rect = element.getBoundingClientRect();
                                    const iframe = window.frameElement;
    
                                    let isVisible = false;
    
                                    if (iframe) {
                                        const iframeRect = iframe.getBoundingClientRect();
                                        isVisible = (
                                            rect.top + iframeRect.top > 0 &&
                                            rect.left + iframeRect.left > 0 &&
                                            rect.bottom + iframeRect.top <= (window.parent.innerHeight || window.parent.document.documentElement.clientHeight) &&
                                            rect.right + iframeRect.left <= (window.parent.innerWidth || window.parent.document.documentElement.clientWidth)
                                        );
                                    } else {
                                        isVisible = (
                                            rect.top > 0 &&
                                            rect.left > 0 &&
                                            rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) &&
                                            rect.right <= (window.innerWidth || document.documentElement.clientWidth)
                                        );
                                    }
    
                                    if (isVisible) {
                                        // console.log("Following products were seen during initial visibility check", product.Product_number)
                                        product.isVisible = true;
                                        if (!product.pricingLoaded) {
                                            this.fetchChunkedPricing([product]);
                                        }
                                    }
                                }
                            });
    
                        },
    
                        //TODO - Improve this for extra user functionality
                        selectPackUnit(product, selectedPackUnit) {
                            Vue.set(product, 'SelectedPackUnit', selectedPackUnit);
                            //console.log('Selected pack unit:', selectedPackUnit);
                            //console.log('Product:', product);
                            product.SelectedPackUnit = selectedPackUnit;
    
                        },
    
                        increaseSelectedQuantity(product) {
                            //console.log('increasing selected quantity for:', product, 'to:', product.Product_Selected_Quantity += 1);
                            product.Product_Selected_Quantity += 1;
                        },
                        decreaseSelectedQuantity(product) {
    
                            if (product.Product_Selected_Quantity > 0) {
                                product.Product_Selected_Quantity -= 1;
                            }
                        },
                        quickAddProductsToChart(productNumber, packageUnit, packageSize, forceRefresh) {
                            //console.log("packageSize - ",packageSize)
                            if (productNumber == undefined)
                                return false;
                            if (productNumber.substring(0, 3) == 'id_')
                                productNumber = $('#' + productNumber);
                            var products = {};
                            products.supplierId = 'WuerthGroup-Wuerth';
                            products.articles = new Array();
                            products.articles.push({
                                productNo: productNumber,
                                packingUnit: packageUnit,
                                quantity: packageSize,
                            });
                            products.campaignName = componentCampaignName;
                            products = JSON.stringify(products);
                            $.ajax({
                                url: `/is-bin/INTERSHOP.enfinity/WFS/${countryId}-B1-Site/${countryMapping[countryId].countryLocale}/-/${countryMapping[countryId].countryCurrency}/ViewModelDetail-AjaxAddArticlesToCurrentShoppingCart`,
                                dataType: 'html',
                                type: 'POST',
    
                                data: {
                                    ShoppingCartForm: products
                                },
                                success: function (F) {
                                    applicationFrameMessages.displayMessage(F);
                                    updateMiniBasketCounter(jsonResponse.shoppingCartItemsCount);
                                    
                                    if (forceRefresh == 1)
                                        location.reload();
                                    return false;
                                }
                            });
                            return false;
    
    
                        },
    
                       
                        initializeSwiper() {
                            this.swiper = new Swiper(".swiper-container.swiper-" + this.mountId, {
                                slidesPerView: 4,
                                spaceBetween: 10,
                                createElements: true,
    
                                pagination: {
                                    el: '.swiper-pagination-' + this.mountId,
                                    dynamicBullets: true,
                                },
                                initialSlide: 0,
                                keyboard: {
                                    enabled: true,
                                },
                                freeMode: true,
                                loop: false,
                                observer: true,
                                observeParents: true,
    
                                breakpoints: {
                                    10: {
                                        slidesPerView: 1,
                                    },
                                    576: {
                                        slidesPerView: 1,
                                    },
                                    768: {
                                        slidesPerView: 2,
                                    },
                                    992: {
                                        slidesPerView: 3,
                                    },
                                    1200: {
                                        slidesPerView: 4,
                                    },
                                },
                                on: {
                                    slideChange: this.debouncedCheckInitialVisibility,
                                    observerUpdate: this.debouncedCheckInitialVisibility
                                }
                            });
                        }
    
                    },
                    mounted() {
                        this.fetchData().then(() => {
                            this.$nextTick(() => {
                                this.checkInitialVisibility();
                                this.createIntersectionObserver();
                                if (loadSwiperVue) {
                                    this.$nextTick(() => {
                                        this.initializeSwiper();
                                        // Call it once after initialization
                                        this.debouncedCheckInitialVisibility();
                                    });
                                } else {
                                    // Call it once if Swiper is not used
                                    this.debouncedCheckInitialVisibility();
                                }
                            });
                        });
                    },
                    created() {
                        this.debouncedCheckInitialVisibility = this.debounce(this.checkInitialVisibility.bind(this), 100);
                        document.addEventListener('custom-promo-active-tab-changed', (event) => {
                            const activeTab = event.detail;
                            const cleanedMountId = mountId.replace('#', '');
                            if (activeTab === cleanedMountId) {
                                this.debouncedCheckInitialVisibility();
                            }
                        });
                    },
                    computed: {
                        getProductPriceForSelectedPackUnit() {
    
                            return (product) => {
                                //console.log("Current Logged in State: ", checkLoggedInState);
    
                                const selectedPackUnit = product.SelectedPackUnit;
                                const matchingPrice = product.WurthPrice.find(price => price.packingUnit === selectedPackUnit);
                                if (product.WurthPrice.length == 0 && product.pricingLoaded) {
                                    return `Error Fetching Price`;
                                }
                                if (matchingPrice && matchingPrice.pricePerPacking !== undefined && matchingPrice.pricePerPacking !== null) {
                                    // console.log("Returning price for matching pack unit");
                                    return `${matchingPrice.pricePerPacking} <sup>ex Vat</sup>`;
                                } else if (product.WurthPrice && product.WurthPrice[0] && product.WurthPrice[0].pricePerPacking !== undefined && product.WurthPrice[0].pricePerPacking !== null) {
                                    //console.log("Returning price for first item in WurthPrice");
                                    return `${product.WurthPrice[0].pricePerPacking} <sup>ex Vat</sup>`;
                                } else if (checkLoggedInState) {
                                    //console.log("Logged in State is true, returning loading gif");
                                    return `<img src="https://eshop.wurth.co.za/is-bin/intershop.static/WFS/WuerthGroup-Wuerth-Site/3128-B1/en_GB/misc/220.gif" alt="Loading" style="display: inline-block; margin: 0 auto;">`;
                                } else {
                                    //console.log("Not logged in, prompting to log in for pricing");
                                    return `Login for Pricing`;
                                }
                            };
                        },
    
    
                    },
    
    
    
                });
    
                app.mount(mountId);
            }
            try{
                window.loadVueScript(initializeApp, loadSwiperVue);
            }
            catch (error) {
                console.error(error.message);
            }
        };
