    const reviews = document.querySelectorAll('.google-reviews-review'); // Get all reviews
    const totalReviews = reviews.length; // Total number of reviews

    // Function to show the next review
    function showNextReview() {
        // Hide the current review
        reviews[currentReviewIndex].classList.remove('active');
        // Move to the next review
        currentReviewIndex = (currentReviewIndex + 1) % totalReviews; // Loop back to start
        // Show the new current review
        reviews[currentReviewIndex].classList.add('active');
    }

